{
  "options": {
    "bindings": "esm",
    "runtime": "incremental",
    "initialMemory": 2,
    "maximumMemory": 2,
    "zeroFilledMemory": true,
    "memoryBase": 16392
  },
  "targets": {
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "outFile": "build/release.wasm",
      "textFile": "build/release.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  }
}